{
  "name": "@esm-bundle/angular__core",
  "version": "15.1.3",
  "description": "Repo to test esm-bundle's automatic publishing",
  "type": "module",
  "exports": {
    ".": {
      "es2015": "./es2015/ivy/angular-core.js",
      "es2020": "./es2020/ivy/angular-core.js",
      "default": "./es2020/ivy/angular-core.js"
    }
  },
  "scripts": {
    "test": "pnpm run build && concurrently -n w: 'pnpm:test:*'",
    "test:browser": "karma start karma.conf.cjs --single-run",
    "debug:test:browser": "karma start karma.conf.cjs",
    "test:formatting": "prettier --check .",
    "build": "rimraf system es2015 es2020 && rollup -c",
    "format": "prettier --write .",
    "release": "release-it",
    "postinstall": "husky install",
    "prepublishOnly": "pinst --disable && pnpm run build",
    "postpublish": "pinst --enable"
  },
  "files": [
    "es2015",
    "es2020",
    "system"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/esm-bundle/angular__core.git"
  },
  "author": "Joel Denning",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/esm-bundle/angular__core/issues"
  },
  "homepage": "https://github.com/esm-bundle/angular__core#readme",
  "devDependencies": {
    "@angular/compiler": "15.1.4",
    "@angular/compiler-cli": "15.1.4",
    "@angular/core": "15.1.4",
    "concurrently": "7.6.0",
    "esm-bundle-scripts": "1.2.0",
    "husky": "8.0.3",
    "karma": "6.4.1",
    "karma-firefox-launcher": "2.1.2",
    "karma-jasmine": "5.1.0",
    "pinst": "3.0.0",
    "prettier": "2.8.4",
    "pretty-quick": "3.1.3",
    "release-it": "15.6.0",
    "release-it-plugin-esm-bundle": "3.0.0",
    "rimraf": "4.1.2",
    "rollup": "2.79.1",
    "rollup-plugin-terser": "7.0.2",
    "rxjs": "7.8.0",
    "typescript": "4.9.5"
  }
}
